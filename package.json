    "validate-commit-msg": "2.11.2",
    "webpack-hot-middleware": "2.17.1"
  },
  "dependencies": {
    "@travi/travi.org-theme": "0.2.0",
    "bootstrap-sass": "3.3.7",
    "material-ui": "0.17.0",
    "microformat-node": "2.0.1",
    "react-bootstrap": "0.30.8",
    "react-dom": "15.4.2",
    "react-router": "3.0.2",
    "react-router-bootstrap": "0.23.1"
  },
