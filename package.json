{
  "name": "@travi/admin.travi.org-components",
  "version": "0.0.0-semantically-released",
  "description": "React components for https://admin.travi.org",
  "scripts": {
    "clean": "rimraf dist/ lib/",
    "copy:scss": "cpy '**/*.scss' '../lib/' --parents --cwd=src/",
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "lint:js": "eslint . .storybook --cache",
    "lint:sass": "sass-lint src/**/*.scss",
    "test": "run-s lint:* test:*",
    "test:unit": "mocha --recursive test/unit -r ./test/helpers/mocha-setup.js",
    "test:integration": "mocha stories/ -r .storybook/mocha-config.js --compilers js:babel-register",
    "coverage": "nyc run-s test:unit",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "start": "start-storybook -p 9002",
    "build:js": "rollup -c",
    "build": "run-s clean build:* copy:scss",
    "build-storybook": "build-storybook -o dist/storybook",
    "deploy": "NODE_DEBUG=gh-pages gh-pages -r \"https://$GH_TOKEN@github.com/travi/admin.travi.org-components.git\" -d dist/storybook/",
    "preversion": "npm test",
    "prepublish": "run-s build",
    "precommit": "npm test",
    "commitmsg": "commitlint -e",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/travi/admin.travi.org-components.git"
  },
  "keywords": [
    "react",
    "storybook",
    "components"
  ],
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/travi/admin.travi.org-components/issues"
  },
  "homepage": "https://github.com/travi/admin.travi.org-components#readme",
  "main": "lib/components.cjs.js",
  "module": "lib/components.es.js",
  "devDependencies": {
    "@storybook/addon-actions": "3.2.12",
    "@storybook/addon-info": "3.2.12",
    "@storybook/addon-links": "3.2.12",
    "@storybook/react": "3.2.12",
    "@travi/any": "1.4.3",
    "@travi/eslint-config-travi": "1.5.11",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-react-remove-prop-types": "0.4.10",
    "babel-plugin-transform-require-ignore": "0.1.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2015-rollup": "3.0.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "cheerio": "1.0.0-rc.2",
    "commitlint-config-travi": "1.2.3",
    "coveralls": "3.0.0",
    "cpy-cli": "1.0.1",
    "css-loader": "0.28.7",
    "cz-conventional-changelog": "2.0.0",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "1.0.1",
    "file-loader": "1.1.5",
    "gh-pages": "1.0.0",
    "glob": "7.1.2",
    "globstar": "1.0.0",
    "husky": "0.14.3",
    "jsdom": "11.3.0",
    "markdownlint-cli": "0.3.1",
    "mocha": "4.0.1",
    "node-sass": "4.5.3",
    "npm-run-all": "4.1.1",
    "nyc": "11.2.1",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-addons-test-utils": "15.6.2",
    "react-dom": "16.0.0",
    "react-helmet": "5.2.0",
    "react-test-renderer": "16.0.0",
    "rimraf": "2.6.2",
    "rollup": "0.50.0",
    "rollup-plugin-babel": "3.0.2",
    "sass-lint": "1.12.0",
    "sass-loader": "6.0.6",
    "semantic-release": "8.2.0",
    "storybook-addon-specifications": "2.1.1",
    "storybook-router": "0.3.1",
    "style-loader": "0.19.0",
    "url-loader": "0.6.2",
    "webpack-hot-middleware": "2.19.1"
  },
  "dependencies": {
    "@travi/travi.org-theme": "0.2.0",
    "bootstrap-sass": "3.3.7",
    "material-ui": "0.19.4",
    "microformat-node": "2.0.1",
    "react-bootstrap": "0.31.4",
    "react-router": "3.2.0",
    "react-router-bootstrap": "0.23.2",
    "react-tap-event-plugin": "3.0.2"
  },
  "peerDependencies": {
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
