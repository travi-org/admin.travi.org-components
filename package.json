{
  "name": "@travi/admin.travi.org-components",
  "version": "0.0.0-semantically-released",
  "description": "React components for https://admin.travi.org",
  "scripts": {
    "clean": "rimraf dist/ lib/",
    "copy:scss": "cpy '**/*.scss' '../lib/' --parents --cwd=src/",
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "lint:js": "eslint .",
    "lint:sass": "sass-lint src/**/*.scss",
    "test": "run-s lint:* test:*",
    "test:unit": "mocha --recursive test/unit",
    "test:integration": "mocha stories/ -r .storybook/mocha-config.js --compilers js:babel-register",
    "coverage": "nyc run-s test:unit",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "start": "start-storybook -p 9002",
    "build:js": "rollup -c",
    "build": "run-s clean build:* copy:scss",
    "build-storybook": "build-storybook -o dist/storybook",
    "deploy": "gh-pages --silent -r \"https://$GH_TOKEN@github.com/travi/admin.travi.org-components.git\" -d dist/storybook/",
    "preversion": "npm test",
    "prepublish": "run-s build",
    "postpublish": "greenkeeper-postpublish",
    "precommit": "npm test",
    "commitmsg": "validate-commit-msg",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/travi/admin.travi.org-components.git"
  },
  "keywords": [
    "react",
    "storybook",
    "components"
  ],
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/travi/admin.travi.org-components/issues"
  },
  "homepage": "https://github.com/travi/admin.travi.org-components#readme",
  "main": "lib/components.cjs.js",
  "module": "lib/components.es.js",
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "3.3.0",
    "@kadira/storybook": "2.35.3",
    "@travi/any": "1.0.4",
    "@travi/eslint-config-travi": "1.1.1",
    "babel-cli": "6.23.0",
    "babel-core": "6.24.0",
    "babel-loader": "6.4.0",
    "babel-plugin-transform-react-remove-prop-types": "0.3.2",
    "babel-plugin-transform-require-ignore": "0.1.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-es2015-rollup": "3.0.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-2": "6.22.0",
    "babel-register": "6.24.0",
    "chai": "3.5.0",
    "cheerio": "0.22.0",
    "coveralls": "2.12.0",
    "cpy-cli": "1.0.1",
    "css-loader": "0.27.3",
    "cz-conventional-changelog": "2.0.0",
    "enzyme": "2.7.1",
    "file-loader": "0.10.1",
    "gh-pages": "0.12.0",
    "glob": "7.1.1",
    "globstar": "1.0.0",
    "greenkeeper-postpublish": "1.0.1",
    "husky": "0.13.3-0",
    "jsdom": "9.12.0",
    "markdownlint-cli": "0.2.0",
    "mocha": "3.2.0",
    "node-sass": "4.5.0",
    "npm-run-all": "4.0.2",
    "nyc": "10.1.2",
    "react": "15.4.2",
    "react-addons-test-utils": "15.4.2",
    "react-helmet": "5.0.0-beta",
    "rimraf": "2.6.1",
    "rollup": "0.41.4",
    "rollup-plugin-babel": "2.7.1",
    "sass-lint": "1.10.2",
    "sass-loader": "6.0.3",
    "semantic-release": "6.3.2",
    "skin-deep": "1.0.0-alpha2",
    "storybook-addon-specifications": "1.0.16",
    "style-loader": "0.14.1",
    "url-loader": "0.5.8",
    "validate-commit-msg": "2.11.2",
    "webpack-hot-middleware": "2.17.1"
  },
  "dependencies": {
    "@travi/travi.org-theme": "0.2.0",
    "bootstrap-sass": "3.3.7",
    "material-ui": "0.17.0",
    "microformat-node": "2.0.1",
    "react-bootstrap": "0.30.8",
    "react-dom": "15.4.2",
    "react-router": "3.0.2",
    "react-router-bootstrap": "0.23.1"
  },
  "peerDependencies": {
    "react": "^15.2.0",
    "react-helmet": "^4.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
