{
  "name": "@travi/admin.travi.org-components",
  "version": "0.11.2",
  "description": "React components for https://admin.travi.org",
  "scripts": {
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "lint:js": "eslint .",
    "lint:sass": "sass-lint src/**/*.scss",
    "test": "run-s lint:* test:*",
    "test:unit": "mocha --recursive test/unit",
    "test:integration": "mocha stories/ -r .storybook/mocha-config.js --compilers js:babel-register",
    "coverage": "nyc run-s test:unit",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "start": "start-storybook -p 9002",
    "build-storybook": "build-storybook -o dist/storybook",
    "deploy": "gh-pages --silent -r \"https://$GH_TOKEN@github.com/travi/admin.travi.org-components.git\" -d dist/storybook/",
    "preversion": "npm test",
    "prepublish": "grunt build",
    "postpublish": "greenkeeper-postpublish",
    "precommit": "npm test",
    "commitmsg": "validate-commit-msg"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/travi/admin.travi.org-components.git"
  },
  "keywords": [
    "react",
    "storybook",
    "components"
  ],
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/travi/admin.travi.org-components/issues"
  },
  "homepage": "https://github.com/travi/admin.travi.org-components#readme",
  "main": "lib/main.js",
  "browser": "lib/browser.js",
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "3.3.0",
    "@kadira/storybook": "2.35.3",
    "@travi/any": "1.0.4",
    "@travi/eslint-config-travi": "1.0.0-beta.4",
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-loader": "6.3.2",
    "babel-plugin-transform-require-ignore": "0.0.2",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-es2015-rollup": "3.0.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-2": "6.22.0",
    "babel-register": "6.23.0",
    "chai": "3.5.0",
    "cheerio": "0.22.0",
    "coveralls": "2.12.0",
    "css-loader": "0.26.2",
    "cz-conventional-changelog": "2.0.0",
    "enzyme": "2.7.1",
    "file-loader": "0.10.1",
    "gh-pages": "0.12.0",
    "glob": "7.1.1",
    "globstar": "1.0.0",
    "greenkeeper-postpublish": "1.0.1",
    "grunt": "1.0.1",
    "grunt-babel": "6.0.0",
    "grunt-cli": "1.2.0",
    "grunt-contrib-clean": "1.0.0",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-watch": "1.0.0",
    "grunt-env": "0.4.4",
    "grunt-notify": "0.4.5",
    "grunt-rollup": "1.0.1",
    "husky": "0.13.2",
    "jsdom": "9.11.0",
    "load-grunt-config": "0.19.2",
    "markdownlint-cli": "0.2.0",
    "mocha": "3.2.0",
    "node-sass": "4.5.0",
    "npm-run-all": "4.0.2",
    "nyc": "10.1.2",
    "react": "15.4.2",
