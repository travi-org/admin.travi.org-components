{
  "name": "@travi/admin.travi.org-components",
  "version": "0.0.0-semantically-released",
  "description": "React components for https://admin.travi.org",
  "scripts": {
    "clean": "rimraf dist/ lib/",
    "copy:scss": "cpy '**/*.scss' '../lib/' --parents --cwd=src/",
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "lint:js": "eslint . .storybook --cache",
    "lint:sass": "sass-lint src/**/*.scss",
    "test": "npm-run-all --print-label --parallel lint:* --parallel test:* --sequential build",
    "test:unit": "nyc run-s test:unit:base",
    "test:unit:base": "mocha --recursive test/unit --require ./test/helpers/mocha-setup.js",
    "test:integration": "mocha stories/ --require @babel/register --require .storybook/mocha-config.js",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "start": "start-storybook -p 9002",
    "prebuild": "run-s clean",
    "build": "npm-run-all --print-label --parallel build:* copy:scss",
    "build:js": "rollup -c",
    "build:storybook": "build-storybook -o dist/storybook --quiet",
    "deploy": "NODE_DEBUG=gh-pages gh-pages -r \"https://$GH_TOKEN@github.com/travi-org/admin.travi.org-components.git\" -d dist/storybook/",
    "prepack": "run-s build",
    "precommit": "npm test",
    "commitmsg": "commitlint -e",
    "semantic-release": "semantic-release"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/travi-org/admin.travi.org-components.git"
  },
  "keywords": [
    "react",
    "storybook",
    "components"
  ],
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/travi-org/admin.travi.org-components/issues"
  },
  "homepage": "https://github.com/travi-org/admin.travi.org-components#readme",
  "main": "lib/components.cjs.js",
  "module": "lib/components.es.js",
  "devDependencies": {
    "@babel/register": "^7.7.7",
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/addon-info": "^3.4.12",
    "@storybook/addon-links": "^5.2.1",
    "@storybook/react": "^5.2.8",
    "@travi/any": "^2.0.4",
    "@travi/babel-preset": "3.0.12",
    "@travi/eslint-config-travi": "^1.8.5",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "0.4.12",
    "babel-plugin-transform-require-ignore": "0.1.1",
    "chai": "^4.2.0",
    "cheerio": "^1.0.0-rc.3",
    "commitlint-config-travi": "^1.3.4",
    "coveralls": "^3.0.6",
    "cpy-cli": "^3.0.0",
    "css-loader": "^3.2.1",
    "cz-conventional-changelog": "^3.0.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "file-loader": "^5.0.1",
    "gh-pages": "^2.1.1",
    "glob": "^7.1.4",
    "globstar": "1.0.0",
    "husky": "^4.0.3",
    "jsdom": "^16.0.1",
    "markdownlint-cli": "^0.21.0",
    "mocha": "^5.0.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.10.1",
    "react-addons-test-utils": "15.6.2",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.1",
    "react-test-renderer": "^16.10.1",
    "rimraf": "^3.0.0",
    "rollup": "^1.22.0",
    "rollup-plugin-babel": "^4.3.3",
    "sass-lint": "^1.13.1",
    "sass-loader": "^6.0.7",
    "storybook-addon-specifications": "^2.1.5",
    "storybook-router": "^0.3.4",
    "style-loader": "^1.0.0",
    "url-loader": "^3.0.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "dependencies": {
    "@travi/travi.org-theme": "0.2.0",
    "bootstrap-sass": "^3.4.1",
    "material-ui": "0.20.0",
    "microformat-node": "2.0.1",
    "react-bootstrap": "0.32.1",
    "react-router": "^3.2.0",
    "react-router-bootstrap": "^0.23.2",
    "react-tap-event-plugin": "^3.0.3"
  },
  "peerDependencies": {
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
