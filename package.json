{
  "name": "@travi/admin.travi.org-components",
  "version": "0.0.0-semantically-released",
  "description": "React components for https://admin.travi.org",
  "scripts": {
    "clean": "rimraf dist/ lib/",
    "copy:scss": "cpy '**/*.scss' '../lib/' --parents --cwd=src/",
    "lint:md": "globstar --node -- markdownlint **/*.md",
    "lint:js": "eslint . .storybook --cache",
    "lint:sass": "sass-lint src/**/*.scss",
    "test": "run-s lint:* test:*",
    "test:unit": "mocha --recursive test/unit -r ./test/helpers/mocha-setup.js",
    "test:integration": "mocha stories/ -r .storybook/mocha-config.js --compilers js:babel-register",
    "coverage": "nyc run-s test:unit",
    "report-coverage": "nyc report --reporter=text-lcov | coveralls",
    "start": "start-storybook -p 9002",
    "build:js": "rollup -c",
    "build": "run-s clean build:* copy:scss",
    "build-storybook": "build-storybook -o dist/storybook",
    "deploy": "NODE_DEBUG=gh-pages gh-pages -r \"https://$GH_TOKEN@github.com/travi/admin.travi.org-components.git\" -d dist/storybook/",
    "preversion": "npm test",
    "prepublish": "run-s build",
    "postpublish": "greenkeeper-postpublish",
    "precommit": "npm test",
    "commitmsg": "validate-commit-msg",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/travi/admin.travi.org-components.git"
  },
  "keywords": [
    "react",
    "storybook",
    "components"
  ],
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/travi/admin.travi.org-components/issues"
  },
  "homepage": "https://github.com/travi/admin.travi.org-components#readme",
  "main": "lib/components.cjs.js",
  "module": "lib/components.es.js",
  "devDependencies": {
    "@storybook/addon-info": "3.0.0",
    "@storybook/react": "3.0.1",
    "@travi/any": "1.0.7",
    "@travi/eslint-config-travi": "1.2.10",
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.5",
    "babel-plugin-transform-require-ignore": "0.1.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2015-rollup": "3.0.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.24.1",
    "chai": "4.0.0",
    "cheerio": "1.0.0-rc.1",
    "coveralls": "2.13.1",
    "cpy-cli": "1.0.1",
    "css-loader": "0.28.4",
    "cz-conventional-changelog": "2.0.0",
    "enzyme": "2.8.2",
    "file-loader": "0.11.2",
    "gh-pages": "1.0.0",
    "glob": "7.1.1",
    "globstar": "1.0.0",
    "greenkeeper-postpublish": "1.0.1",
    "husky": "0.13.4",
    "jsdom": "11.0.0",
    "markdownlint-cli": "0.3.1",
    "mocha": "3.4.2",
    "node-sass": "4.5.3",
    "npm-run-all": "4.0.2",
    "nyc": "11.0.2",
    "prop-types": "15.5.10",
    "react": "15.5.4",
    "react-addons-test-utils": "15.5.1",
    "react-helmet": "5.1.3",
    "react-test-renderer": "15.5.4",
    "rimraf": "2.6.1",
    "rollup": "0.42.0",
    "rollup-plugin-babel": "2.7.1",
    "sass-lint": "1.10.2",
    "sass-loader": "6.0.5",
    "semantic-release": "6.3.6",
    "skin-deep": "1.0.0",
    "storybook-addon-specifications": "1.1.0",
    "storybook-router": "0.2.6",
    "style-loader": "0.18.2",
    "url-loader": "0.5.8",
    "validate-commit-msg": "2.12.1",
    "webpack-hot-middleware": "2.18.0"
  },
  "dependencies": {
    "@travi/travi.org-theme": "0.2.0",
    "bootstrap-sass": "3.3.7",
    "material-ui": "0.18.1",
    "microformat-node": "2.0.1",
    "react-bootstrap": "0.31.0",
    "react-dom": "15.5.4",
    "react-router": "3.0.5",
    "react-router-bootstrap": "0.23.2"
  },
  "peerDependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.5.0",
    "react-helmet": "^5.1.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
